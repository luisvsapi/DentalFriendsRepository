  <script src="../../../scripts/session.js"></script>
</head>

<body>

  <div class="modal fade" id="modalMedicalResume" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title">Detalles</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div id="modalBodyMedicalResume" class="modal-body">
          <p>Descripción</p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-info" data-dismiss="modal">Cerrar</button>
        </div>
      </div>
    </div>
  </div>

  <!-- ======= Header ======= -->
  <%- include('./headerUser', {active: "medicalResume" }); %>
    <!-- End Header -->

    <main id="main">
      <!-- ======= Portfolio Section ======= -->
      <section id="contact" class="contact">
        <div class="container" data-aos="fade-up">

          <div class="section-title">
            <h2>Resumen Médico de Pacientes</h2>
          </div>

          <div class="mt-5 mt-lg-0">
            <!--mt-5 mt-lg-0"-->
            <form id="searchMedicalResume" action="/medicalResume" method="post" role="form">
              <div class="form-row">
                <label class="col-form-label">Buscar Paciente:</label>
                <div class="col-md-5 form-group col-8">
                  <input class="form-control" id="filterMedicalResume"
                    placeholder="Cédula/NombrePaciente/CorreoPaciente" maxlength="10" minlength="3" type="text">
                </div>
                <div class="col-md-2 col-3">
                  <button id="buttonSearchMedicalResume" type="submit" class="btn btn-info">Buscar</button>
                </div>
                <div class="col-md-2"></div>
              </div>
            </form><br>
          </div>

          <div class="row">
            <table id="medicalResumeTable" class="table">
              <tr>
                <th id="idMedicalResume">Id</th>
                <th id="dateMedicalResume">Fecha</th>
                <th id="pacientMedicalResume">Paciente</th>
                <th id="pacientDetailsMedicalResume" class="d-flex justify-content-center">Detalles</th>
              </tr>

              <% if (resume) { %>
                <% console.log(resume) %>
                  <% for(var i=0; i < resume.length; i++) { %>
                    <% if (resume[i] !=null) { %>
                      <tr>
                        <td>
                          <%= resume[i].date %>
                        </td>
                        <td>
                          <%= resume[i].idUser %>
                        </td>
                      </tr>
                      <% } %>
                        <% } %>
                          <% } %>
            </table>
          </div>
        </div>
      </section><!-- End Portfolio Section -->

    </main><!-- End #main -->

    <!-- ======= Footer ======= -->
    <%- include('./footer', {}); %>

      <!-- Vendor JS Files -->
      <script src="../assets/vendor/jquery/jquery.min.js"></script>
      <script src="../assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
      <script src="../assets/vendor/jquery.easing/jquery.easing.min.js"></script>
      <script src="../assets/vendor/php-email-form/validate.js"></script>
      <script src="../assets/vendor/waypoints/jquery.waypoints.min.js"></script>
      <script src="../assets/vendor/counterup/counterup.min.js"></script>
      <script src="../assets/vendor/owl.carousel/owl.carousel.min.js"></script>
      <script src="../assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
      <script src="../assets/vendor/venobox/venobox.min.js"></script>
      <script src="../assets/vendor/aos/aos.js"></script>

      <!-- Template Main JS File -->
      <script src="../assets/js/main.js"></script>
      <script src="../scripts/alertify.min.js"></script>
      <script src="../scripts/utils.js"></script>
      <script src="../scripts/medicalResume.js"></script>
      <script src="../scripts/session.js"></script>
      <!-- <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script> -->



</body>

</html>